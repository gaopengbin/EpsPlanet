{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "es2020",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types",
      "src/typings",
      "src/typing.d.ts"
    ],
    "types": [
      "lodash",
      "cesium"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "epsgis": [
        "dist/epsgis"
      ],
      "epsgis/*": [
        "dist/epsgis/*"
      ],
      "arcgis2d": [
        "dist/epsgis/arcgis2d"
      ],
      "arcgis2d/*": [
        "dist/epsgis/arcgis2d/*"
      ],
      "dist/epsgis/arcgis3d": [
        "dist/epsgis/arcgis3d"
      ],
      "dist/epsgis/arcgis3d/*": [
        "dist/epsgis/arcgis3d/*"
      ],
      "supermap3d": [
        "dist/supermap3d"
      ],
      "supermap3d/*": [
        "dist/supermap3d/*"
      ],
      "epsgisoa": [
        "dist/epsgisoa"
      ],
      "epsgisoa/*": [
        "dist/epsgisoa/*"
      ],
      "epsplanet": [
        "dist/epsplanet"
      ],
      "epsplanet/*": [
        "dist/epsplanet/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true,
    "experimentalDecorators": true
  }
}